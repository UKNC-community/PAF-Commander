	.TITLE	PPUNL
	.DSABL	GBL
	.GLOBL	.PUT	.FREE
	.GLOBL	LO$BEG	LO$END

	.GLOBL	PP$WAIT
	.GLOBL	C.UNLO

C1CSR	=	176664
C1DAT	=	176666

.MACRO	.SEND	BYTE
	MOVB	BYTE,@#C1DAT
.ENDM

.MACRO	.WAIT
	CALL	PP$WAIT
.ENDM

PP$UNL::
	.WAIT
	.SEND	#C.UNLO
.C1CSR	==	. + 2
	MOVB	#0,@#C1CSR
PP$ADR	==	. + 2
	MOV	#0,-(SP)
	MOV	#<LO$END-LO$BEG>/2,-(SP)
	CALL	.FREE
OLD$KB	==	. + 2
	MOV	#0,@#60

	RETURN

	.END
